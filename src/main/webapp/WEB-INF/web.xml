<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>JFuture task</display-name>

  <!--Dynamics page-->
  <servlet>
    <servlet-name>dynamics</servlet-name>
    <jsp-file>/WEB-INF/views/dynamics.jsp</jsp-file>
  </servlet>

  <!--Top directors page-->
  <servlet>
    <servlet-name>directors</servlet-name>
    <jsp-file>/WEB-INF/views/directors.jsp</jsp-file>
  </servlet>


  <servlet-mapping>
    <servlet-name>dynamics</servlet-name>
    <url-pattern>/dynamics</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>directors</servlet-name>
    <url-pattern>/directors</url-pattern>
  </servlet-mapping>

</web-app>